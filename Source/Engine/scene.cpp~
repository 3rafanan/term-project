-- PROTOTYPE void marx::Scene::onLoad
--
-- NOTES: this function
-- 
-----------------------------------------------------------------------------*/
Scene Scene::onLoad()
{
	if(instance != NULL)
		return new Secne();
	else
		return instance;
}

