# Makefile for engine

$CC=g++



test: Scene.o AppWindow.o
	$(CC) AppWindow.o -o sfml-app -L ../../SFML/lib -L /usr/lib64 -lsfml-graphics -lsfml-window -lsfml-system -std=c++11

clean:
	rm -f *.o *.bak test

Scene.o:
	$(CC) -c Scene.cpp -I ../../SFML/include -std=c++11

AppWindow.o:
	$(CC) -c AppWindow.cpp -I ../../SFML/include -std=c++11
	
